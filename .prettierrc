{
  "tabWidth": 2,
  "useTabs": false,
  "plugins": ["prettier-plugin-astro"],
  "overrides": [
    {
      "files": ".astro",
      "options": {
        "parser": "astro"
      }
    }
  ],
    "trailingComma": "es5",
    "semi": true,
    "singleQuote": true,
    "bracketSpacing": true,
    "eslintIntegration": true
}
